readme	:	guessinggame.sh
			touch	README.md
			echo	"Title of the project : PEER GRADED ASSIGNMENT"	>	README.md
			echo "\n Date and Time at which make was ran"	>>	README.md
			date	>>	README.md
			echo	"\nNumber of lines file guessinggame.sh contains:"	>>	README.md
			cat	guessinggame.sh	|	wc	-l	>>	README.md


